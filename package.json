{
  "name": "csfe-snippets",
  "displayName": "csfe-snippets",
  "description": "csfe-snippets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:csfe-snippets.insertLink",
    "onCommand:csfe-snippets.insertFigure"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "csfe configuration",
      "type": "object",
      "properties": {
        "csfe.imagePathTemplate": {
          "type": "string",
          "default": "/images/${year}/${month}/",
          "description": "Specifies the folder containing images."
        },
        "csfe.filePathTemplate": {
          "type": "string",
          "default": "/file/${year}/${month}/",
          "description": "Specifies the folder containing files."
        }
      }
    },
    "commands": [
      {
        "command": "csfe-snippets.insertLink",
        "title": "Insert Link"
      },
      {
        "command": "csfe-snippets.insertFigure",
        "title": "Insert Figure"
      }
    ],
    "keybindings": [
      {
        "command": "csfe-snippets.insertLink",
        "key": "shift+ctrl+l",
        "mac": "shift+cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "csfe-snippets.insertFigure",
        "key": "shift+ctrl+i",
        "mac": "shift+cmd+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
